    {
      "title": "Chicken Fried Rice",
      "type": "recipe",
      "url": "",
      "image": "images/chicken_fried_rice.jpg",
      "tags": [
        "chicken"
      ],
      "ingredients": [
        "3 boneless skinless chicken breasts",
        "1 18 oz. bottle Teriyaki sauce",
        "3 tbsp. vegetable oil",
        "1 cup peas and carrots",
        "1 onion chopped",
        "2 tsp. minced garlic",
        "2 eggs slightly beaten",
        "3 cups cooked white rice (day old or leftover cold rice is best)",
        "3-4 tbsp. soy sauce"
      ],
      "steps": [
        "cook chicken covered in teriyaki sauce in a crock-pot on high heat for 3 hours. OR in an oven at 375 F for 35 minutes",
        "once cooked cut into small pieces and set aside",
        "heat the oil in a large skillet over medium high heat",
        "add the peas carrots, onion, and garlic",
        "stir fry until tender",
        "lower the heat to medium low and push the mixture to one side of the pan",
        "pour eggs on the other side of the skillet",
        "stir fry until scrambled",
        "now add rice and soy sauce and blend together well",
        "stir in cooked chicken and cook until heated through"
      ],
