    {
      "title": "Sausage Egg and Cheese Breakfast Rollups",
      "type": "recipe",
      "url": "",
      "image": "images/sausage_egg_cheese_rollups.jpg",
      "tags": [],
      "ingredients": [
        "5 eggs",
        "1 can (8 oz.) crescent dinner rolls",
        "8 cooked breakfast sausage links",
        "4 slices Cheddar cheese",
        "salt and pepper to taste"
      ],
      "steps": [
        "heat oven to 350 F",
        "crack and beat eggs",
        "reserve 2 tbsp. beaten eggs for brushing on crescent rolls",
        "cook remaining eggs until scrambled on stovetop",
        "unroll dough and separate into 8 triangles",
        "cut cheese slices in half and place half on each triangle",
        "top each with scrambled eggs and sausage",
        "loosely roll up",
        "place on ungreased cookie sheet",
        "brush reserved beaten egg on top of each crescent",
        "sprinkle salt and pepper over each",
        "bake 15-18 minutes or until golden brown"
      ],
